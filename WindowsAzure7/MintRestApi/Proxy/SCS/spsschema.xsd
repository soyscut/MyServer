        <xs:element ref="PUIDLow" />
        <xs:element ref="NumberOfUnits" />
        <xs:element ref="UnitsOfMeasure" />
        <xs:element ref="Description" />
        <xs:element ref="UsageCategory" minOccurs="0" />
