  <xs:element name="BillingMode">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0" />
        <xs:maxLength value="128" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="ExternalId">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0" />
        <xs:maxLength value="128" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="ItemEntrySet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ItemEntry" minOccurs="1" maxOccurs="900" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="ItemEntry">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ItemExternalId" />
        <xs:element ref="ItemName" />
        <xs:element ref="ItemDescription" />
        <xs:element ref="ItemCategory" />
        <xs:element ref="ItemType" />
        <xs:element ref="RevenueSKU" minOccurs="0" maxOccurs="1" />
        <xs:element ref="ProductType" minOccurs="0" maxOccurs="1" />
        <xs:element name="UnitPrice" type="xs:decimal" />
        <xs:element name="Quantity" type="xs:int" />
        <xs:element name="IsTaxIncluded" type="xs:boolean" />
        <xs:element ref="TaxEntrySet" minOccurs="0" maxOccurs="1" />
        <xs:element ref="ItemAttributeSet" minOccurs="0" maxOccurs="1" />
        <xs:element name="IsTaxExempt" type="xs:boolean" minOccurs="0" maxOccurs="1" default="false" />
        <xs:element name="ThirdPartyReseller" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <xs:element name="ConfigurationGroup" type="xs:string" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="ItemAttributeSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ItemAttribute" minOccurs="0" maxOccurs="50" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="ItemAttribute">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Namespace" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="32" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Name" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="32" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Value" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="512" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="ItemExternalId">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1" />
        <xs:maxLength value="128" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="ItemType">
    <xs:simpleType>
      <xs:restriction base="xs:NMTOKEN">
        <xs:enumeration value="PURCHASE" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="ItemName">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1" />
        <xs:maxLength value="128" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="ItemDescription">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0" />
        <xs:maxLength value="512" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="ItemCategory">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0" />
        <xs:maxLength value="128" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="ProductType">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0" />
        <xs:maxLength value="32" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="TaxEntrySet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="TaxEntry" minOccurs="0" maxOccurs="unbounded" />
        <xs:element name="TotalTaxAmount" type="xs:decimal" />
        <xs:element name="TotalTaxRate" type="xs:decimal" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="TaxEntry">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Jurisdiction" type="taxType" />
        <xs:element name="TaxAmount" type="xs:decimal" />
        <xs:element name="TaxRate" type="xs:decimal" />
        <xs:element ref="Exempt" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="OrderReceipt">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="OrderId" type="xs:long" />
        <xs:element name="OrderPaymentStatus" type="xs:string" />
        <xs:element name="OrderShippingStatus" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="TotalAmount" type="xs:decimal" />
        <xs:element name="TotalTaxAmount" type="xs:decimal" />
        <xs:element name="IsTaxIncluded" type="xs:boolean" />
        <xs:element ref="PaymentInstrumentId" minOccurs="0" />
        <xs:element ref="CorrelationInfo" minOccurs="0" />
        <xs:element ref="OrderAttributeSet" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="CorrelationInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="DisplayInfo" />
        <xs:element ref="CorrelationData" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="DisplayInfo">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0" />
        <xs:maxLength value="256" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="CorrelationData">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0" />
        <xs:maxLength value="256" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="TaxCalculationInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="TaxExemptionInfo" minOccurs="0" maxOccurs="1" />
        <xs:element ref="AddressInfo" minOccurs="0" maxOccurs="1" />
        <xs:element ref="BillingPartnerInfo" minOccurs="0" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="BillingPartnerInfo">
    <xs:complexType>
        <xs:sequence>
            <xs:element name="BillingPartnerName" type="xs:string" minOccurs="1" maxOccurs="1" /> 
        </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="TaxExemptionInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="CertificateNumber">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="64" />
              <!-- check this 1 -->
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Type">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="32" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Status" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="32" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="ExpDate" minOccurs="0">
          <xs:simpleType>
            <xs:union>
              <xs:simpleType>
                <xs:restriction base="dateType" />
              </xs:simpleType>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:length value="0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:union>
          </xs:simpleType>
        </xs:element>
        <xs:element name="DateAdded" minOccurs="0">
          <xs:simpleType>
            <xs:union>
              <xs:simpleType>
                <xs:restriction base="dateType" />
              </xs:simpleType>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:length value="0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:union>
          </xs:simpleType>
        </xs:element>
        <xs:element name="DateReceived" minOccurs="0">
          <xs:simpleType>
            <xs:union>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:length value="10" />
                </xs:restriction>
              </xs:simpleType>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:length value="0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:union>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- CalculateTax / SubmitOrder / RefundOrder ends here -->

  <!-- BillingReference starts here-->
  <xs:element name="BillingReferenceInfoSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="BillingReferenceInfo" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="BillingReferenceInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="BillingReferenceId" />
        <xs:element ref="PaymentInstrumentId" />
        <xs:element name="CountryCode" type="countryCodeType" />
        <xs:element ref="Currency" />
        <xs:element ref="PaymentMethodType" />
        <xs:element ref="BillingConfigGroup" minOccurs="0" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="BillingReferenceId" type="idType" />

  <xs:element name="PaymentMethodType">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0" />
        <xs:maxLength value="255" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="BillingConfigGroup">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1" />
        <xs:maxLength value="256" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <!-- BillingReference ends here-->


  <!-- SendValidationTokenInfo starts here -->

  <xs:element name="ValidationToken">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ValidationTokenType" minOccurs="1" maxOccurs="1" />
        <xs:element ref="MobilePhoneValidationToken" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="ValidationTokenType">
    <xs:simpleType>
      <xs:restriction base="xs:NMTOKEN">
        <xs:enumeration value="MOBILEPHONE" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="MobilePhoneValidationToken">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="MessageText" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="300" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:simpleType name="XMLString">
    <xs:restriction base="xs:string">
      <xs:pattern value="[\u0009\u000A\u000D\u0020-\uD7FF\uE000-\uFFFD\u10000-\u10FFFF]*" />
    </xs:restriction>
  </xs:simpleType>
  <!-- SendValidationTokenInfo ends here -->

  <!-- Flexible Offering starts here-->
  <xs:element name="OverrideAmount">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Amount"                 minOccurs="1" maxOccurs="1"                   /> 
        <xs:element name="IsTaxIncluded"         minOccurs="0" maxOccurs="1" type="xs:boolean" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ConversionExtraInformation">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="CreditMode"            minOccurs="1" maxOccurs="1"  type="CreditModeType"/>
        <xs:element name="TimeConversionRatio"   minOccurs="0" maxOccurs="1"  type="timeConversionRatioType" />
        <xs:element name="RoundBoundary"         minOccurs="0" maxOccurs="1"  type="roundBoundaryType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:simpleType name="CreditModeType">
    <xs:restriction  base="xs:string">
      <xs:enumeration value="CreditByRefund" />
      <xs:enumeration value="CreditByTimeExtension" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="timeConversionRatioType"  >
    <xs:restriction base="xs:decimal" >
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="roundBoundaryType" >
    <xs:restriction  base="xs:positiveInteger">
      <xs:maxInclusive value="30"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="TimeExtensionInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RemainingDaysinOldSubs" minOccurs="1" maxOccurs="1"  type="xs:unsignedInt"/>
        <xs:element name="TimeConversionRatio"    minOccurs="1" maxOccurs="1"  type="timeConversionRatioType" />
        <xs:element name="RoundBoundary"          minOccurs="1" maxOccurs="1"  type="roundBoundaryType"/>
        <xs:element name="MonthExtended"          minOccurs="1" maxOccurs="1"  type="xs:unsignedInt"/>
        <xs:element name="SubscriptionEndDate"    minOccurs="1" maxOccurs="1"  type="nillableDateTimeType"  />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="RatingEventSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="RatingEvent"  minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="UniqueEventName">
      <xs:selector xpath="bdk:RatingEvent" />
      <xs:field xpath="@type" />
    </xs:unique>
  </xs:element>
  <xs:element name="RatingEvent">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="RatingRule"   minOccurs="1" maxOccurs="unbounded" />
      </xs:sequence>
      <xs:attribute name="type"  use="required" type="OverrideEventType"/>
    </xs:complexType>
    <xs:unique name="UniqueRuleStartEndCombination">
      <xs:selector xpath="bdk:RatingRule"/>
      <xs:field xpath="@type"/>
      <xs:field xpath="bdk:CycleStart"/>
      <xs:field xpath="bdk:CycleEnd"/>
    </xs:unique>
  </xs:element>
  <xs:simpleType name="OverrideEventType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CancelSubscription" />
      <xs:enumeration value="ConvertFrom" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="RatingRule">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref ="Amount"      minOccurs="1" maxOccurs="1"                  />
        <xs:element name="CycleStart"  minOccurs="1" maxOccurs="1" type="CycleType" />
        <xs:element name="CycleEnd"    minOccurs="1" maxOccurs="1" type="CycleType" />
        <xs:element name="CycleUnit"   minOccurs="1" maxOccurs="1" type="CycleType" />
      </xs:sequence>
      <xs:attribute name="type"    use="required" type="OverrideRuleType"/>
      <xs:attribute name="guidref" use="required" type="guidType" />
    </xs:complexType>
  </xs:element>
  <xs:simpleType name="OverrideRuleType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Proration" />
      <xs:enumeration value="Charge" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CycleType" >
    <xs:restriction base="xs:positiveInteger" />
  </xs:simpleType>
  <!-- Flexible Offering end here-->

  <!--Reseller Inclusion start here-->
  <xs:simpleType name="PKPNType">
    <xs:restriction base="xs:string">
      <xs:pattern value="^[a-zA-Z0-9]{3}-[a-zA-Z0-9]{5}$" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ChannelSKUType">
    <xs:restriction base="xs:string">
      <xs:pattern value="^[a-zA-Z0-9]{3}-[0-9]{5}$" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="RevenueSKUType">
    <xs:restriction base="xs:string">
      <xs:pattern value="^[a-zA-Z0-9]{3}-[0-9]{5}$" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ResellerConfigType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PKPN" />
      <xs:enumeration value="ChannelSKU" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="GetConfigType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="equals" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="dateTimeType">
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{4}-\d\d-\d\dT\d\d:\d\d(:\d\d(\.\d{0,9})?)?" />
    </xs:restriction>
  </xs:simpleType>
  <!-- PartnerConfigObjectXML element -->
  <xs:element name="PartnerConfigObjectXML">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PartnerConfigObjectType" type="ResellerConfigType" minOccurs="1" maxOccurs="1"/>
        <xs:choice minOccurs="1" maxOccurs="1">
          <xs:element ref="PKPN"/>
          <xs:element ref="ChannelSKU"/>
        </xs:choice>
       </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- PKPN element -->
  <xs:element name="PKPN">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ID" type="PKPNType" minOccurs="1" />
        <xs:element name="ExpirationDate" type="dateTimeType" />
        <xs:element name="IsMarketing" type="xs:boolean"/>
        <xs:element name="SourcePartner" type="guidType" minOccurs="1" maxOccurs="1"/>
        <xs:element name="ChannelType" type="xs:string" minOccurs="1" maxOccurs="1"/>
        <xs:element name="TokenType" type="xs:string" minOccurs="1" maxOccurs="1"/>
        <xs:element name="TokenPreSetValueList" minOccurs="0"  maxOccurs="1" >
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="TokenPreSetValue" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- TokenPreSetValue element -->
  <xs:element name="TokenPreSetValue">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Currency" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:length value="3" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="PreSetValue">
          <xs:simpleType>
            <xs:restriction base="xs:decimal">
              <xs:minInclusive value="0.0"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="CSVExpireDuration" type="xs:int" nillable ="true" minOccurs="0" maxOccurs="1" />
        <xs:element name="DurationTypeId" type="xs:unsignedByte" nillable ="true" minOccurs="0" maxOccurs="1" />
        <xs:element name="IsPromotional" type="xs:boolean" minOccurs="0" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- ChannelSKU element -->
  <xs:element name="ChannelSKU">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PartnerID" type="guidType" minOccurs="1" maxOccurs="1"/>
        <xs:element name="ID" type="ChannelSKUType" minOccurs="1" maxOccurs="1"/>
        <xs:element name="PKPNSetList">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="PKPNSet" minOccurs="1" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- PKPNSet element -->
  <xs:element name="PKPNSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PKPNID" type="PKPNType" minOccurs="1" maxOccurs="1"/>
        <xs:element name="PercentageAllocation" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:double">
              <xs:minInclusive value="0.0"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="BOMQuantity">
          <xs:simpleType>
            <xs:restriction base="xs:int">
              <xs:minInclusive value="0"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="MinPriceUSD">
          <xs:simpleType>
            <xs:restriction base="xs:double">
              <xs:minInclusive value="0.0"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="MaxPriceUSD">
          <xs:simpleType>
            <xs:restriction base="xs:double">
              <xs:minInclusive value="0.0"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- PartnerConfiguratoinResponse element -->
  <xs:element name="PartnerConfigurationResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PartnerConfigObjectType" type="ResellerConfigType" minOccurs="1" maxOccurs="1"/>
        <xs:element name="ResultDetails" type="xs:string"/>
        <xs:element name="Version" type="xs:int"/>
        <xs:element ref="PartnerConfigQueryResultSet"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- PartnerConfigQueryResultSet element -->
  <xs:element name="PartnerConfigQueryResultSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="PartnerConfigQueryResult" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- PartnerConfigQueryResult element -->
  <xs:element name="PartnerConfigQueryResult">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="PartnerConfigObjectXML"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- PartnerConfigQuery element -->
  <xs:element name="PartnerConfigQuery">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="QueryType" type="GetConfigType"/>
        <xs:element name="PartnerConfigObjectType" type="ResellerConfigType" minOccurs="1" maxOccurs="1"/>
        <xs:element name="Value" type="xs:string" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- TokenInfoXML element -->
  <xs:element name="ResellerTokenInfoXML">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PKPN" type="PKPNType" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="InputTokenList"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- InputTokenList element -->
  <xs:element name="InputTokenList">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ResellerToken" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- ResellerToken element -->
  <xs:element name="ResellerToken">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ID" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="1" />
              <xs:maxLength value="64" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="MaxUsage" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:int">
              <xs:minInclusive value="0"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
