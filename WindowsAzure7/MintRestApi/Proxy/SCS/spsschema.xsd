            <xs:sequence minOccurs="0">
              <xs:element ref="ReferralType" />
              <xs:element ref="ReferralData" />
              <xs:element name="PartnerName" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="64" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PartnerId" type="guidType" minOccurs="0" />
