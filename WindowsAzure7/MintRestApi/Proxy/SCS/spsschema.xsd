  <xs:element name="Gross" type="xs:decimal" />
  <xs:element name="Charge" type="xs:decimal" />
  <xs:element name="TaxType">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="3" />
        <xs:maxLength value="15" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <!-- "TaxAmount" element  -->
  <xs:element name="TaxAmount" type="xs:string" />
  <!-- xs:float -->
  <xs:element name="TaxAmountNG" type="xs:string" />
  <!-- xs:float -->
  <!-- "TaxRate" element  -->
  <xs:element name="TaxRate" type="xs:string" />
  <!-- xs:float -->
  <!-- "Exempt" element  -->
  <xs:element name="Exempt">
    <xs:simpleType>
      <xs:restriction base="xs:NMTOKEN">
        <xs:enumeration value="TRUE" />
        <xs:enumeration value="FALSE" />
        <xs:enumeration value="true" />
        <xs:enumeration value="false" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <!-- "AmountCharged" element  -->
  <xs:element name="AmountChargedSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="AmountCharged" minOccurs="1" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="AmountCharged">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Gross" />
        <xs:element ref="Charge" minOccurs="0" />
        <xs:element name="Locale">
          <!-- convertsubscription may return empty locale -->
          <xs:simpleType>
            <xs:union>
              <xs:simpleType>
                <xs:restriction base="xs:string" />
              </xs:simpleType>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:length value="0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:union>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Currency">
          <xs:simpleType>
            <xs:union>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:length value="3" />
                  <xs:pattern value="^[A-Z]{3}$" />
                </xs:restriction>
              </xs:simpleType>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:length value="0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:union>
          </xs:simpleType>
        </xs:element>
        <xs:element ref="AppliedDiscountSet" minOccurs="0" />
        <xs:element name="TaxSet" type="taxSet25Type" minOccurs="0" />
        <xs:element name="ChargeStatus" type="chargeStatusType" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- "AppliedDiscountSet" element  -->
  <xs:element name="AppliedDiscountSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="AppliedDiscount" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
