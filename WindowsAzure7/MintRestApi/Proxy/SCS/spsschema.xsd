      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- "RoleAssignment" element  -->
  <xs:element name="RoleAssignment">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Role" />
        <xs:element ref="PUIDHigh" />
        <xs:element ref="PUIDLow" />
        <xs:element ref="PMN" minOccurs="0" />
        <xs:element ref="IdentityType" minOccurs="0" />
        <xs:element ref="IdentityValue" minOccurs="0" />
        <xs:element ref="RightSet" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Permit -->
  <!-- This is output schema, maybe we should fix this empty value. -->
  <!-- "ServiceInstanceStatus" element  -->
  <xs:element name="ServiceInstanceStatus">
    <xs:simpleType>
      <xs:restriction base="xs:int">
        <xs:enumeration value="2" />
        <xs:enumeration value="3" />
        <xs:enumeration value="4" />
        <xs:enumeration value="5" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <!-- "ServiceComponentName" element  -->
  <xs:element name="ServiceComponentName">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0" />
        <xs:maxLength value="128" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <!-- "SellingTenantId" element  -->
  <xs:element name="SellingTenantId" type="guidType" />
  <!--   -->
  <!-- "ServiceInstancePermit" element  -->
  <!-- Several nodes differ from the other ServiceInstance definition which is why this is defined separately  -->
  <xs:complexType name="serviceInstancePermitType">
    <xs:sequence>
      <xs:element ref="ServiceInstanceId" />
      <xs:element ref="ServiceInstanceFriendlyName" />
      <xs:element ref="ServiceInstanceStatus" />
      <xs:element ref="ProvisioningStatus" />
      <xs:element ref="ProvisioningErrorCode" />
      <xs:element ref="ServiceComponentId" />
      <xs:element ref="ServiceComponentName" />
      <xs:element name="PassportBits" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Value" type="xs:string" />
            <!-- 65538-->
            <xs:element name="Mask" type="xs:string" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PartnerName" type="xs:string" />
      <xs:element name="PartnerId" type="guidType" />
      <xs:element name="GotoBAMDate" type="dateType" minOccurs="0" />
      <xs:element name="Evict" type="xs:boolean" minOccurs="0" />
      <xs:element name="IsPMNBased" type="xs:boolean" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <!-- "Permit" element  -->
  <xs:element name="Permit">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PermitCreationTime" type="nillableDateTimeType" minOccurs="0" />
        <xs:element name="ObjectType">
          <xs:simpleType>
            <xs:restriction base="xs:NMTOKEN">
              <xs:enumeration value="Account" />
              <xs:enumeration value="Subscription" />
              <xs:enumeration value="ServiceInstance" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element ref="RoleAssignment" />
        <xs:choice minOccurs="0">
          <xs:element name="Account">
            <xs:complexType>
              <xs:sequence minOccurs="0">
                <xs:element ref="AccountId" />
                <xs:element ref="AccountStatus" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="Subscription">
            <xs:complexType>
              <xs:sequence minOccurs="0">
                <xs:element ref="SubscriptionId" />
                <xs:element ref="SubscriptionStatus" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="ServiceInstance" type="serviceInstancePermitType" />
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- "PermitSet" element  -->
  <xs:element name="PermitSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Permit" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PermitHistorySet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="PermitHistory" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PermitHistory">
