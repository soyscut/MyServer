  </xs:element>
  <!-- Referrals  -->
  <xs:element name="ReferralType">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1" />
        <xs:maxLength value="256" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="ReferralData">
    <xs:complexType mixed="true">
      <xs:sequence>
