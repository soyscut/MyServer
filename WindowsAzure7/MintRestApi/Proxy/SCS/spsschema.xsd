        <xs:enumeration value="CompletedUI" />
        <xs:enumeration value="Authorization" />
        <xs:enumeration value="Rejection" />
        <xs:enumeration value="Reversal" />
        <xs:enumeration value="MSCancel" />
        <xs:enumeration value="MerchantReversalComplete" />
        <xs:enumeration value="MerchantReversalRejection" />
        <xs:enumeration value="MSReversal" />
        <xs:enumeration value="MSCancelReversal" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="RejectionReasonCode">
    <xs:simpleType>
      <xs:union>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="TimedOut" />
            <xs:enumeration value="Cancelled" />
            <xs:enumeration value="AuthFailed" />
            <xs:enumeration value="StopPayment" />
            <xs:enumeration value="InactiveAccount" />
