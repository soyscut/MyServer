  <!-- "Usage" element  -->
  <xs:element name="Usage">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Timestamp" />
        <xs:element ref="EventId" />
        <xs:element name="ObjectId" type="nillableIdType" />
