  <!-- "UserContextInfo" element -->
  <xs:element name="UserContextInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="IPAddress">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="15"/>
              <xs:minLength value="7" />
              <xs:pattern value="^(\d{1,3}\.){3}\d{1,3}$" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="BrowserType" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="40"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="BrowserEmail" minOccurs="0">
          <xs:simpleType>
            <xs:union>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:minLength value="6" />
                  <xs:maxLength value="256" />
                  <xs:pattern value="^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$" />
                </xs:restriction>
              </xs:simpleType>
              <xs:simpleType>
                <xs:restriction base="xs:string">
