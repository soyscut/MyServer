<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
           targetNamespace="urn:schemas-microsoft-com:billing-data"
           xmlns:bdk="urn:schemas-microsoft-com:billing-data"
           xmlns="urn:schemas-microsoft-com:billing-data" 
           elementFormDefault="qualified" 
           version="1.0">
  <xs:annotation>
    <xs:documentation>
      XSD schema for SCS APIs
    </xs:documentation>
  </xs:annotation>
  <xs:element name="AccountInfoSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="AccountInfo" minOccurs="0" maxOccurs="unbounded" />
        <!-- ActiveFreeTrialForPUID is returned by GetAccountIdFromAdminPUID in case user has an ENABLED or EXPIRED free trial subscription -->
        <xs:element name="ActiveFreeTrialForPUID" type="xs:boolean" minOccurs="0" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="AccountInfo">
    <xs:complexType>
      <xs:all>
        <xs:element ref="AccountId" minOccurs="0" />
        <xs:element ref="AccountStatusInfo" minOccurs="0" />
        <xs:element name="AccountCreationDate" type="nillableDateTimeType" minOccurs="0" />
        <xs:element name="AccountHistoryDate" type="nillableDateTimeType" minOccurs="0" />
        <xs:element name="AccountSource" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="32" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="LegacyAccountId" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="16" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element ref="AnniversaryDate" minOccurs="0" />
        <xs:element name="LastPeriodProcessed" type="xs:unsignedInt" minOccurs="0" />
        <xs:element name="CustomerType" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="32" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="DefaultAddressId" type="nillableIdType" minOccurs="0" />
        <xs:element ref="FriendlyName" minOccurs="0" />
        <xs:element ref="FirstName" minOccurs="0" />
        <!-- minOccurs = 0 To honor SearchAccount -->
        <xs:element name="FirstNamePronunciation" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="64" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element ref="LastName" minOccurs="0" />
        <!-- minOccurs = 0 To honor SearchAccount -->
        <xs:element name="LastNamePronunciation" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="64" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Email" minOccurs="0">
          <xs:simpleType>
            <xs:union>
              <xs:simpleType>
                <xs:restriction base="XMLString">
                </xs:restriction>
              </xs:simpleType>
              <xs:simpleType>
                <xs:restriction base="XMLString">
                  <xs:length value="0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:union>
          </xs:simpleType>
        </xs:element>
        <xs:element name="CompanyName" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="64" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="CompanyNamePronunciation" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="64" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element ref="Locale" minOccurs="0" />
        <!-- Use 0 to honor SearchAccount -->
        <xs:element ref="Currency" minOccurs="0" />
        <!-- Use 0 to honor SearchAccount -->
        <!-- Not using countrycode ref as not yet decided if we want to allow empty nodes-->
        <xs:element name="CountryCode" minOccurs="0">
          <xs:simpleType>
            <xs:union>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:length value="2" />
                  <xs:pattern value="^[a-zA-Z]{2}$" />
                </xs:restriction>
              </xs:simpleType>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:length value="0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:union>
          </xs:simpleType>
        </xs:element>
        <xs:element name="CorporateIdentity" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="32" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="CorporateVatId" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="25" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="CorporateLegalEntity" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="128" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element ref="CorporateAddress" minOccurs="0" />
        <xs:element name="HCI" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="5" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="HCIValid" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="1" />
              <xs:maxLength value="5" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="PhoneSet" minOccurs="0">
          <xs:complexType>
            <xs:sequence minOccurs="0" maxOccurs="unbounded">
              <xs:element ref="Phone" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element ref="AddressInfoSet" minOccurs="0" />
        <xs:element name="TaxExemptionInfoSet" minOccurs="0">
          <xs:complexType>
            <xs:sequence minOccurs="0" maxOccurs="2">
              <!-- change from unbounded to 2-->
              <xs:element name="TaxExemptionInfo">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="TECertificateNumber">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:minLength value="0" />
                          <xs:maxLength value="64" />
                          <!-- check this 1 -->
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="TEType">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:minLength value="0" />
                          <xs:maxLength value="32" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="TEStatus" minOccurs="0">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:minLength value="0" />
                          <xs:maxLength value="32" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="TEExpDate" minOccurs="0">
                      <xs:simpleType>
                        <xs:union>
                          <xs:simpleType>
                            <xs:restriction base="dateType" />
                          </xs:simpleType>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:length value="0" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:union>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="TEDateAdded" minOccurs="0">
                      <xs:simpleType>
                        <xs:union>
                          <xs:simpleType>
                            <xs:restriction base="dateType" />
                          </xs:simpleType>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:length value="0" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:union>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="TEDateReceived" minOccurs="0">
                      <xs:simpleType>
                        <xs:union>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:length value="10" />
                            </xs:restriction>
                          </xs:simpleType>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:length value="0" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:union>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="TEIsSkipViesValidation" minOccurs="0">
                      <xs:simpleType>
                        <xs:restriction base="xs:boolean" />
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element ref="SubscriptionInfoSet" minOccurs="0" maxOccurs="1" />
        <xs:element name="AccountAttributeInfoSet" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="AttributeInfo" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Permit" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="PermitCreationTime" type="nillableDateTimeType" minOccurs="0" />
              <xs:element name="RoleAssignment">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="Role" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element ref="PendingTransactionExists" minOccurs="0" maxOccurs="1" />
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:element name="AddressInfoSet">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="AddressInfo" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="AddressInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="AddressId" minOccurs="0" />
        <xs:element ref="FriendlyName" />
        <xs:element name="UnitNumber" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="15" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Street1">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="128" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Street2" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="128" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Street3" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="128" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="City" type="cityType" />
        <xs:element name="District" type="districtType" minOccurs="0" />
        <xs:element name="State" type="stateType" minOccurs="0" />
        <xs:element name="CountryCode" type="countryCodeType" minOccurs="0" />
        <!-- to honor SearchAccount -->
        <xs:element name="PostalCode" type="postalCodeType" minOccurs="0" />
        <xs:element name="AddressMapAttempted" type="xs:boolean" minOccurs="0" />
        <xs:element name="AddressMapSucceeded" type="xs:boolean" minOccurs="0" />
        <xs:element name="AddressMapFailureReason" type="xs:unsignedByte" minOccurs="0" />
        <xs:element name="AddressMapConfidenceScore" type="xs:double" minOccurs="0" />
        <xs:element name="ManualAddressSpecified" type="xs:boolean" minOccurs="0" />
        <!-- to honor Concept of Tenant, Syndication and Order -->
        <xs:element name="FirstName" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="255" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="LastName" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="255" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="ExternalAddressId" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="XMLString">
              <xs:minLength value="0" />
              <xs:maxLength value="255" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CorporateAddress">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="CorporateStreet1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="128" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="CorporateStreet2" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="128" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="CorporateStreet3" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="128" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="CorporateCity" type="cityType" />
        <xs:element name="CorporateDistrict" type="districtType" minOccurs="0" />
        <xs:element name="CorporateState" type="stateType" minOccurs="0" />
        <xs:element name="CorporateCountryCode" type="countryCodeType" minOccurs="0" />
        <!-- to honor SearchAccount -->
        <xs:element name="CorporatePostalCode" type="postalCodeType" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:simpleType name="cityType">
    <xs:restriction base="XMLString">
      <xs:minLength value="0" />
      <xs:maxLength value="64" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="districtType">
    <xs:restriction base="XMLString">
      <xs:minLength value="0" />
      <xs:maxLength value="64" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="stateType">
    <xs:restriction base="XMLString">
      <xs:minLength value="0" />
      <!-- check it, change to 0 from 1 to honor kr -->
      <xs:maxLength value="64" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="postalCodeType">
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="16" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="AccountStatementInfoSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="AccountStatementInfo" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:all>
              <xs:element name="BillingPeriodSet">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="BillingPeriod" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="PeriodStatus">
                            <xs:simpleType>
                              <xs:restriction base="xs:NMTOKEN">
                                <xs:enumeration value="OPEN" />
                                <xs:enumeration value="CLOSED" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="HasActivity" type="xs:boolean" />
                          <xs:element name="HasPayoutActivity" type="xs:boolean" />
                          <xs:element name="BeginDate" type="dateType" />
                          <xs:element name="EndDate" type="dateType" />
                        </xs:sequence>
                        <xs:attribute name="idRef" type="billingPeriodIdType" use="required" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element ref="UnBilledUsageEventSet" />
              <xs:element ref="AccountStatementSet" />
            </xs:all>
            <xs:attribute name="accountIdRef" type="idType" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="AccountReferralInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="AccountInfo" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="AccountId" minOccurs="1" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="ReferredToSet" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ReferredTo" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ReferredToAccountInfo" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element ref="AccountId" minOccurs="1" maxOccurs="1" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element ref="ReferredToSubscriptionInfo" minOccurs="1" maxOccurs="1" />
                    <xs:element ref="ReferralProgram" minOccurs="1" maxOccurs="1" />
                    <xs:element ref="ReferralEventSet" minOccurs="0" maxOccurs="unbounded" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="ReferredBySet" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ReferredBy" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ReferredByAccountInfo" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element ref="AccountId" minOccurs="1" maxOccurs="1" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element ref="ReferredToSubscriptionInfo" minOccurs="1" maxOccurs="1" />
                    <xs:element ref="ReferralProgram" minOccurs="1" maxOccurs="1" />
                    <xs:element ref="ReferralEventSet" minOccurs="0" maxOccurs="unbounded" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ReferredToSubscriptionInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SubscriptionId" minOccurs="1" maxOccurs="1" />
        <xs:element ref="OfferingGuid" minOccurs="1" maxOccurs="1" />
        <xs:element ref="OfferingDescription" minOccurs="1" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ReferralProgram">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ProgramGuid" type="guidType" minOccurs="1" maxOccurs="1" />
        <xs:element name="ProgramDescription" type="internalOfferingDescriptionType" minOccurs="1" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ReferralEventSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ReferralEvent" minOccurs="1" maxOccurs="unbounded">
          <xs:complexType>
            <xs:all>
              <xs:element name="EventType" type="internalOfferingDescriptionType" minOccurs="1" maxOccurs="1" />
              <xs:element name="EventClass" type="internalOfferingDescriptionType" minOccurs="1" maxOccurs="1" />
              <!-- Not doing anything here...Should we restrict to varbinary -->
              <xs:element name="EventDetails" minOccurs="0" maxOccurs="1" />
              <xs:element name="InsertedDate" type="dateTime" minOccurs="1" maxOccurs="1" />
            </xs:all>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="UnBilledUsageEventSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="SubscriptionEventSet" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="SubscriptionInfo" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="SubscriptionId" />
                    <xs:element ref="OfferingGUID" />
                    <xs:element name="AllowNonCSRCancel" type="xs:boolean" minOccurs="0" />
                    <xs:element ref="ResourceBillingMethod" minOccurs="0" />
                    <xs:element ref="ProductGuid" minOccurs="0" />
                    <xs:element name="ProductName" type="offeringDescriptionType" minOccurs="0" />
                    <xs:element name="ProductDescription" type="offeringDescriptionType" minOccurs="0" />
                    <xs:element ref="ProductClassGuid" minOccurs="0" />
                    <xs:element ref="SubscriptionDescription" />
                    <xs:element ref="SubscriptionPriceDescription" />
                    <xs:element ref="InternalSubscriptionDescription" />
                    <xs:element ref="FriendlyName" />
                    <xs:element ref="PaymentInstrumentId" />
                    <xs:element ref="Currency" />
                    <xs:element ref="SupportOfferingDescription" />
                    <xs:element ref="SupportOfferingPriceDescription" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="EventSet" type="eventSetType" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="eventSetType">
    <xs:sequence minOccurs="0">
      <xs:element name="ResourceDescription" minOccurs="0">
        <xs:complexType mixed="true">
          <xs:complexContent>
            <xs:restriction base="xs:anyType">
              <xs:attribute name="idRef" use="required" type="uuidType" />
            </xs:restriction>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="Units">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="UnitOfMeasure" type="unitType" />
            <xs:element name="NumberOfUnitsApplied" type="nillableIntType" />
            <!-- GetStatement may return empty -->
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element ref="IsPrePaidUsage" minOccurs="0" />
      <xs:element name="Event" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="EventType">
              <xs:complexType>
                <xs:attribute name="idRef" use="required" type="xs:positiveInteger" />
              </xs:complexType>
            </xs:element>
            <xs:element name="Date" type="dateTime" />
            <xs:element name="Description" type="xs:string" />
            <xs:element name="NumberOfUnits" type="xs:int" />
