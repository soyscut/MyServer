      <xs:enumeration value="MANUAL_REFUND" />
      <xs:enumeration value="CYCLE_ADJUSTMENT" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="capitalBooleanType">
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="FALSE" />
      <xs:enumeration value="TRUE" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="nillableCapitalBooleanType">
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="capitalBooleanType" />
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:length value="0" />
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="offeringDescriptionType">
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="1000" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="offeringPriceDescriptionType">
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="1000" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="internalOfferingDescriptionType">
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="512" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="chargeStatusType">
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="SUCCESS" />
      <xs:enumeration value="FAIL" />
      <xs:enumeration value="PENDING" />
      <xs:enumeration value="COMPUTE" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="subscriptionStatusType">
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:NMTOKENS">
          <xs:enumeration value="PENDING" />
          <xs:enumeration value="ENABLED" />
          <xs:enumeration value="SUSPENDED" />
          <xs:enumeration value="CANCELED" />
          <xs:enumeration value="EXPIRED" />
          <xs:enumeration value="ABANDONED" />
          <xs:enumeration value="CONVERTED" />
          <xs:enumeration value="PENDINGCOMMITCREATESUBS" />
          <xs:enumeration value="PENDINGCOMMITCONVERTSUBS" />
          <xs:enumeration value="Pending" />
          <xs:enumeration value="Enabled" />
          <xs:enumeration value="Suspended" />
          <xs:enumeration value="Canceled" />
          <xs:enumeration value="Expired" />
          <xs:enumeration value="Abandoned" />
          <xs:enumeration value="PendingCommitCreateSubs" />
          <xs:enumeration value="PendingCommitConvertSubs" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:NMTOKEN">
          <xs:enumeration value="1" />
          <xs:enumeration value="2" />
          <xs:enumeration value="3" />
          <xs:enumeration value="4" />
          <xs:enumeration value="5" />
          <xs:enumeration value="6" />
          <xs:enumeration value="7" />
          <xs:enumeration value="8" />
          <xs:enumeration value="9" />
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="transactionStatusType">
    <xs:restriction base="xs:NMTOKENS">
      <xs:enumeration value="INITIAL" />
      <xs:enumeration value="AUTHORIZATION_IN_PROGRESS" />
      <xs:enumeration value="FULFILLED" />
      <xs:enumeration value="CANCELLED" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BrandId">
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="128" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TransactionID">
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="32" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="URL">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="512" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="MerchantRefNumberType">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="20" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ECIType">
    <xs:restriction base="xs:string">
      <xs:length value="1" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="PaymentInstrumentData">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="CreditCardData" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="WholesaleData" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ExtraInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="PaymentInstrumentData" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CreditCardData">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PayerAuthCode" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:choice>
              <xs:element name="VerifiedByVisa">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="ECI" type="ECIType" />
                    <xs:element name="CAVV">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:length value="40" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="MasterCardSecureCode">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="ECI" type="ECIType" />
                    <xs:element name="UCAF">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:length value="32" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
            </xs:choice>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="WholesaleData">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="EncryptedPassword" minOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="250" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ChargeInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="OfferingGUID" minOccurs="0" maxOccurs="1" />
        <xs:element ref="SubscriptionId" minOccurs="0" maxOccurs="1" />
        <xs:element ref="ItemInstanceId" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Amount" minOccurs="1" maxOccurs="1" />
        <xs:element ref="Currency" minOccurs="1" maxOccurs="1" />
        <!-- Note: the actual length of Description + SupportContactInfo is much smaller
                     and payment method type dependent (e.g. 22 or 13 for credit card. Also not
                     all characters are allowed (e.g. only ]A-Za-z0-9 ] for cerdit card -->
        <xs:element name="Description" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="3500" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="SupportContactInfo" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="3500" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="ImmediateSettle" minOccurs="1" maxOccurs="1" type="xs:boolean" />
        <xs:element ref="CommentInfo" minOccurs="0" maxOccurs="1" />
        <xs:element name="OriginalMerchantRefNumber" minOccurs="0" maxOccurs="1" type="MerchantRefNumberType" />
        <xs:element name="TransactionType" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="CHARGE" />
              <xs:enumeration value="CREDIT" />
              <xs:enumeration value="PAYOUT" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:simpleType name="ValidationResult">
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="VALID" />
      <xs:enumeration value="INVALID" />
      <xs:enumeration value="DECLINE" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="PIValidationResults">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ResultCode" type="ValidationResult" minOccurs="1" maxOccurs="1" />
        <xs:element ref="PaymentInstrumentResults" minOccurs="1" maxOccurs="1" />
        <xs:element ref="RiskCheckResults" minOccurs="0" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="RiskCheckResults">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ResultCode" type="xs:string" minOccurs="1" maxOccurs="1" />
        <xs:element name="CriteriaCodes" type="xs:string" minOccurs="0" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PaymentInstrumentResults">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="MerchantRefNumber" minOccurs="0" maxOccurs="1" type="MerchantRefNumberType" />
        <xs:element ref="PaymentInstrumentType" minOccurs="1" maxOccurs="1" />
        <xs:element ref="CreditCardResult" minOccurs="0" maxOccurs="1" />
        <xs:element ref="PayPalPayinResult" minOccurs="0" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CreditCardResult">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="AVSResultCode" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="1" />
              <xs:maxLength value="1" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="CVMResultCode" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="1" />
              <xs:maxLength value="1" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="AuthorizationResultCode" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="8" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="SettlementResultCode" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="2" />
              <xs:maxLength value="2" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PayPalPayinResult">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RedirectUrl" type="xs:string" minOccurs="1" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="BankAccountInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="AccountHolderName" minOccurs="0" maxOccurs="1" />
        <xs:element name="BankName" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="50" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="City" type="cityType" minOccurs="0" maxOccurs="1" />
        <xs:element name="BankAccountNumber" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="64" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element ref="BankCode" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="BranchCode" minOccurs="0" maxOccurs="1" />
        <xs:element ref="CheckDigits" minOccurs="0" maxOccurs="1" />
        <xs:element name="CustomerReadableBankInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="CustomerReadableBankInfoLine" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute name="label" type="xs:string" use="optional"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PaymentResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="64" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="VendorInternalTransactionID" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="64" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element ref ="PaymentInstrumentType" minOccurs="1" maxOccurs="1" />
        <xs:element name="Amount" type="xs:decimal" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Currency" minOccurs="0" maxOccurs="1" />
        <xs:element ref="PaymentInstructions" minOccurs="0" maxOccurs="1" />
        <xs:element name="PaymentResult" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="ResultCode"  minOccurs="1" maxOccurs="1"/>
              <xs:element ref="ResultDetail" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PaymentInstructions">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PaymentReferenceNumber" type="xs:string" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="ResultCode">
    <xs:simpleType>
      <xs:restriction base="xs:NMTOKEN">
        <xs:enumeration value="RECEIVED" />
        <xs:enumeration value="RECEIVE_FAILED" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="ResultDetail">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ReasonCode" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:NMTOKEN">
              <xs:enumeration value="TRANSACTION_ALREADY_CANCELLED" />
              <xs:enumeration value="CUSTOMER_PROFILE_INFO_INVALID" />
              <xs:enumeration value="PAYMENT_INSTRUMENT_INFO_INVALID" />
              <xs:enumeration value="TRANSACTION_AMOUNT_INFO_INVALID" />
              <xs:enumeration value="TRANSACTION_DESCRIPTION_INVALID" />
              <xs:enumeration value="UNKNOWN_PROCESSING_FAILURE" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Message" minOccurs="0" maxOccurs="unbounded">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="0" />
              <xs:maxLength value="128" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="UserInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PassportMemberName" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="6" />
              <xs:maxLength value="129" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Country" minOccurs="1" maxOccurs="1" type="countryCodeType" />
        <xs:element ref="Locale" minOccurs="1" maxOccurs="1" />
        <xs:element name="Email" minOccurs="1" maxOccurs="1" type="EmailType" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="PayUserResults">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ConfirmationId" minOccurs="1" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="1" />
              <xs:maxLength value="128" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="PayoutPreference">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="OfferingGuid" minOccurs="1" maxOccurs="1" type="xs:string" />
        <xs:element name="PaymentMethodId" minOccurs="1" maxOccurs="1" type="xs:string" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="ServiceProfile">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ServiceInstanceId" type="idType" />
        <xs:element ref="ServiceComponentId"/>
        <xs:element name="PreferredPayoutPaymentInstrumentId" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:length value="16" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element ref="ServiceComponentName" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="UserProfileInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="SCSAccountId" type="idType"  minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="PayoutActivitySet">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PayoutActivity" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="OfferingInfo" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="OfferingGUID" minOccurs="0" />
                    <xs:element ref="OfferingDescription" minOccurs="0" />
                    <xs:element ref="InternalOfferingDescription" minOccurs="0" />
                    <xs:element ref="ProductGuid" minOccurs="0" />
                    <xs:element name="ProductName" type="offeringDescriptionType" minOccurs="0"  />
                    <xs:element name="ProductDescription" type="offeringDescriptionType" minOccurs="0" />
                    <xs:element ref="ProductClassGuid" minOccurs="0" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="LineItemSet" type="lineItemSetType" minOccurs="0" maxOccurs="1" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="CallerInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="DelegateId" type="xs:unsignedLong" minOccurs="0" maxOccurs="1" />
        <xs:element name="RequesterId" type="xs:unsignedLong" minOccurs="0" maxOccurs="1" />
        <xs:element name="OnBehalfOfPartner" type="guidType" minOccurs="0" maxOccurs="1" />
        <xs:element name="IdentityType" minOccurs="0" maxOccurs="1">
          <xs:simpleType>
            <xs:restriction base="xs:NMTOKEN">
              <xs:enumeration value="PUID" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <!-- TODO: Add CallerInfoContext for future extension-->
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- CalculateTax / SubmitOrder /RefundOrder starts here -->

  <xs:element name="OrderDetail">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Timestamp" type="nillableDateTimeType" minOccurs="0" maxOccurs="1" />
        <xs:element ref="BillingMode" minOccurs="0" maxOccurs="1" />
        <xs:element ref="ExternalId" />
        <xs:element ref="Description" />
        <xs:element ref="Currency" />
        <xs:element name="OriginalOrderId" type="xs:long" minOccurs="0" maxOccurs="1" />
        <xs:element ref="ItemEntrySet" minOccurs="0" />
        <xs:element ref="OrderAttributeSet" minOccurs="0" maxOccurs="1" />
