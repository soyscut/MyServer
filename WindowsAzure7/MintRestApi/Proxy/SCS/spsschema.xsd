          <xs:complexType>
            <xs:sequence>
              <xs:element name="Date" type="nillableDateTimeType" />
              <xs:element name="Type" type="xs:positiveInteger" />
              <xs:element name="ByPUIDHigh" type="nillableIntType" />
              <xs:element name="ByPUIDLow" type="nillableIntType" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
